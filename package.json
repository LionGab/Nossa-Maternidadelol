  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx server/index.ts",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "type-check": "tsc",
    "dev:clean": "node -e \"require('fs').rmSync('node_modules/.vite', {recursive: true, force: true}); require('fs').rmSync('dist', {recursive: true, force: true})\" && npm run dev",
    "analyze": "npm run build && npx vite-bundle-visualizer --open",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "vercel-build": "npm run build",
    "generate:session-secret": "node scripts/generate-session-secret.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext .ts,.tsx --max-warnings 0",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "clean": "rm -rf dist node_modules/.vite *.tsbuildinfo",
    "clean:all": "npm run clean && rm -rf node_modules/.cache .turbo",
    "setup": "node scripts/setup.js",
    "postinstall": "node scripts/postinstall.js"
  },
